{
  "name": "inovanti/rsa-validator",
  "description": "Package for validation and decryption using RSA public keys",
  "type": "library",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0",
    "illuminate/database": "*",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Inovanti\\RSAValidator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Inovanti\\RSAValidator\\Providers\\RSAValidatorServiceProvider"
      ],
      "aliases": {
        "RSAValidator": "Inovanti\\RSAValidator\\Facades\\RSAValidator"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  }
}
