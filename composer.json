{
  "name": "inovanti-bank/rsa-validator",
  "description": "Package for validation and decryption using RSA public keys (Private Key and Public Key)",
  "type": "library",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "InovantiBank\\RSAValidator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "InovantiBank\\RSAValidator\\Providers\\RSAValidatorServiceProvider"
      ],
      "aliases": {
        "RSAValidator": "InovantiBank\\RSAValidator\\Facades\\RSAValidator"
      }
    }
  },
  "minimum-stability": "dev",
